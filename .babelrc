{
	"presets": ["babel-preset-niksy/next", "babel-preset-niksy"],
	"plugins": ["@babel/plugin-transform-object-assign"],
	"env": {
		"test": {
			"presets": [
				[
					"babel-preset-niksy",
					{
						"@babel/preset-env": {
							"useBuiltIns": "usage",
							"corejs": 2
						}
					}
				]
			],
			"plugins": [
				[
					"@babel/plugin-transform-runtime",
					{
						"corejs": false,
						"helpers": true,
						"regenerator": false,
						"useESModules": true
					}
				]
			]
		}
	}
}
